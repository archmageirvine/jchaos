<!-- Filter for excluding warnings we are not interested in -->
<FindBugsFilter>

     <!-- Exclude all classes in java namespace -->
     <Match classregex="java.*"/>

     <!-- Exclude code generated by ANTLR4 -->
     <Match classregex="irvine.math.expression.GrammarLexer"/>
     <Match classregex="irvine.math.expression.GrammarParser"/>

     <!-- Some tests we will skip for all classes -->
     <!-- SnVI not needed because we do not use serialization for persistence -->
     <!-- PZLA is a stupid test about returning null arrays -->
     <Match classregex=".*">
        <BugCode name="SnVI,PZLA,BC"/>
     </Match>

     <!-- fb-contrib classes we don't want -->
     <Match classregex=".*">
        <BugCode name="ACEM,BL,CBX,CC,CFS,CLI,DMC,DSOC,EXS,FCCD,FPL,IMC,ITC,ITU,JVR,MDM,MOM,MRC,NOS,OCP,OPM,PCOA,PIS,PL,PMB,PRMC,PSC,S508C,SCII,SEC,SLS,UMTP"/>
     </Match>
     <Match classregex=".*">
        <Bug pattern="IOI_USE_OF_FILE_STREAM_CONSTRUCTORS,MDM_THREAD_YIELD,STT_STRING_PARSING_A_FIELD,STT_TOSTRING_STORED_IN_FIELD,STT_TOSTRING_MAP_KEYING"/>
     </Match>
     <Match classregex=".*Test">
        <BugCode name="FPL,PCAIL,RFI,SACM,SPP"/>
     </Match>
     <Match classregex=".*TestUtils">
        <BugCode name="RFI"/>
     </Match>

     <Match classregex=".*Test">
       <BugCode name="SIC"/>
     </Match>

     <Match classregex=".*">
        <Bug pattern="DM_DEFAULT_ENCODING"/>
     </Match>

     <!-- This test generating lots of false positives for 1.7 autoclose -->
     <Match classregex=".*">
        <Bug pattern="NP_LOAD_OF_KNOWN_NULL_VALUE"/>
     </Match>
     <!-- Apparent false positives -->
     <Match classregex="irvine.language.Kwic" >
       <BugCode name="RCN" />
     </Match>

     <!-- Skip unreferenced field in anonymous classes -->
     <Match classregex=".*\$[0-9]+">
       <BugCode name="UrF"/>
     </Match>

     <!-- Intentional uses of System.exit() -->
     <Match classregex="irvine.util.CliFlags\$1" >
       <BugPattern name="DM_EXIT" />
     </Match>
     <Match classregex="chaos.graphics.HandleExit">
       <BugPattern name="DM_EXIT" />
     </Match>
     <Match classregex="irvine.util.graphics.Plasma\$1">
       <BugPattern name="DM_EXIT" />
     </Match>
     <Match classregex="irvine.util.graphics.ParticleExplosion\$1">
       <BugPattern name="DM_EXIT" />
     </Match>

     <!-- Math code making careful use of equality on floating-point -->
     <Match classregex="irvine.twiddle.Crunch">
       <BugCode name="FE"/>
     </Match>
     <Match classregex="irvine.twiddle.PrimitiveTimings">
       <BugCode name="FE"/>
     </Match>
     <Match classregex="irvine.euler.Euler184">
       <BugCode name="FE"/>
     </Match>
     <Match classregex="irvine.euler.Euler247\$Square">
       <BugCode name="FE"/>
     </Match>
     <Match classregex="irvine.euler.Euler461">
       <BugCode name="FE"/>
     </Match>
     <Match classregex="irvine.solve.Puzzle007">
       <BugCode name="FE"/>
     </Match>
     <Match classregex="irvine.util.LimitedLengthPriorityQueue">
       <BugCode name="FE"/>
     </Match>

     <!-- Intentional naked notify -->
     <Match classregex="chaos.util.BooleanLockTest">
       <BugCode name="NN"/>
     </Match>

     <Match classregex="irvine.math.PowerSetTest">
       <BugCode name="ITA"/>
     </Match>

     <!-- Intentional null -->
     <Match classregex="irvine.util.io.DedupeTest">
       <BugCode name="NP"/>
     </Match>

     <!-- False positive for file closing -->
     <Match classregex="irvine.util.HashCollider">
       <BugCode name="OS"/>
     </Match>

     <!-- Deliberate use of shortcut expression evaluation without expression -->
     <Match classregex="irvine.factor.factor.Cleaver">
       <BugCode name="UCF"/>
     </Match>

     <!-- Fairly ugly, but needs to be able to be modified from outside -->
     <Match classregex="irvine.util.graphics.RadioButton">
       <BugCode name="EI2"/>
     </Match>

     <!-- Special return -->
     <Match classregex="chaos.selector.OrdinarySelector">
       <BugCode name="EI"/>
     </Match>

     <!-- Too many dependencies -->
     <Match classregex="chaos.graphics.GenericScreen\$2">
       <BugCode name="SIC"/>
     </Match>

     <!-- This class is never serialized anyway -->
     <Match classregex="chaos.graphics.ImageList">
       <BugCode name="Se"/>
     </Match>

     <!-- Fingbugs false-positive -->
     <Match classregex="irvine.solve.Puzzle009">
       <BugCode name="IM"/>
     </Match>
     <Match classregex="irvine.euler.Euler539">
       <BugCode name="BIT"/>
     </Match>
     <Match classregex="irvine.factor.gnfs.VectorTimesVectorTest">
       <BugCode name="BSHIFT"/>
     </Match>

     <!-- I am allowed to ignore return values and have absolute paths in a test -->
     <Match classregex=".*Test">
       <BugCode name="RV"/>
     </Match>
     <Match classregex=".*Test">
       <BugCode name="DMI"/>
     </Match>

     <Match classregex="irvine.tgraph.DependencyGraph">
       <BugCode name="IICU"/>
     </Match>

     <Match classregex="chaos.util.BooleanLockTest">
       <BugCode name="FCBL"/>
     </Match>

     <Match classregex="chaos.graphics.Animator\$1">
       <BugCode name="STS"/>
     </Match>

     <Match classregex="irvine.util.CliFlags\$Flag">
       <BugCode name="LEST,UCPM,URV"/>
     </Match>

     <Match classregex="irvine.util.io.IOUtils">
       <BugCode name="LEST"/>
     </Match>
     <Match classregex="irvine.language.LiquidTokenizer">
       <BugCode name="LEST"/>
     </Match>

     <!-- false-positives? -->                                                                           <Match classregex="irvine.solve.Puzzle084">
       <BugCode name="SEO"/>
     </Match>
     <Match classregex="irvine.factor.factor.FibonacciFactorizer">
       <BugCode name="PCAIL"/>
     </Match>
     <Match classregex="irvine.euler.Euler44">
       <BugCode name="PCAIL"/>
     </Match>

     <!-- We have some iterators over infinite sets.  In those circumstances
          this is a false positive.  Also, it is really the programmers job
          to test this. -->
     <Match>
       <Bug pattern="IT_NO_SUCH_ELEMENT" />
     </Match>

     <!-- Whole point of this method is to allow external statement. -->
     <Match classregex="irvine.totara.TotaraConnection">
       <BugCode name="SQL"/>
     </Match>
</FindBugsFilter>
